#include<bits/stdc++.h>
#define fast cin.tie(0);cout.tie(0);
#define ll long long int
#define endl "\n"
#define n 20
using namespace std;
int main(){
    fast;
   
    for(int i=1;i<n;i++){
        int f=0;
        for(int j=2;j<i;j++){
             if(i%j==0){
                 f=1;
                 break;
             }
            
        }
        if(f==0)
           cout<<i<<endl;
    }


}



==============================================

#include<bits/stdc++.h>
#define fast cin.tie(0);cout.tie(0);
#define ll long long int
#define endl "\n"
using namespace std;
int main(){
    fast;
   
 for(int i=0;i<5;i++){
     int c=1;
     for(int j=4;j>i;j--)
         cout<<" ";
     for(int j=0;j<=i;j++){
         cout<<c<<" ";
         c=(c*(i-j)/(j+1));
     }
     cout<<endl;
 }

}


    1
   1 1
  1 2 1
 1 3 3 1
1 4 6 4 1


